            byte    $64 {brown gnd},$E0 {1st purple},$E1,$E2
            byte    $E3,$F3,$FA,$FE
            byte    $FF,$DF,$7F,$1F
t_palette1  byte    $FF,$3B,$37,$33 {OCEAN?}           ' Tile color palette 1
            byte    $2F,$9F,%00000000,%00000000 {?}
            byte    $2F,$33,$37,$3B { WATERFALL}
            byte    $68,$64,$60,%00000000 {ROCKS}

